<h1>Servicios</h1>
<%= link_to "New servicio", new_servicio_path, class:"btn btn-primary my-3" %>
<div id="servicios">
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Nombre</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
        <% @servicios.each do |servicio| %>
          <tr class="">
            <td><%= servicio.nombre%></td>
            <td><%= link_to "Show this servicio", servicio %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<%== pagy_bootstrap_nav(@pagy) %>
