<h1 class="mb-4">Medical Historials</h1>

<div id="medical_historials">
  <% @medical_historials.each do |medical_historial| %>
    <div class="card mb-3">
      <div class="card-body">
        <%= render medical_historial %>
        <p class="mt-3">
          <%= link_to "Show this medical historial", medical_historial, class: "btn btn-primary" %>
        </p>
      </div>
    </div>
  <% end %>
</div>

<div class="mt-3">
  <%= link_to "New Medical Historial", new_mascotum_medical_historial_path, class: "btn btn-success" %>
</div>
